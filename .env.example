# Twilio Configuration for Phone Verification
# Get these from your Twilio Console: https://console.twilio.com/

# Your Twilio Account SID
VITE_TWILIO_ACCOUNT_SID=your_account_sid_here

# Your Twilio Auth Token
VITE_TWILIO_AUTH_TOKEN=your_auth_token_here

# Your Twilio Verify Service SID (create one in Twilio Console)
VITE_TWILIO_VERIFY_SERVICE_SID=your_verify_service_sid_here

# Optional: Your Twilio Phone Number (for custom SMS)
VITE_TWILIO_PHONE_NUMBER=+1234567890

# Firebase Configuration (existing)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id